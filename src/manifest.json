{
    "name": "Facebook Video Speed Control",
    "version": "1.0",
    "description": "Speed up or slow down playback of Facebook videos",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["https://*.facebook.com/*"],
            "js": ["content-script.js"]
        }
    ],
    "manifest_version": 2
}
